<configuration>
  <appender name="FILE" class="ch.qos.logback.core.FileAppender">
    <file>digdag-profiler.log</file>
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS Z} [%level] (%thread\) %class: %m%n</pattern>
    </encoder>
  </appender>

  <root level="DEBUG">
    <appender-ref ref="FILE"/>
  </root>
</configuration>
